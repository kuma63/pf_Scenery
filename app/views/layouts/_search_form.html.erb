<%= form_with url: search_path, local: true, method: :get do |f| %>
 <%= f.text_field :word, placeholder: "検索", class: "bi bi-search" %>
 <%= f.select :range, options_for_select([['User'], ['Photo']]) %>
 <%= f.hidden_field :search, :value => "partial_match" %>
 <%= button_tag :type => "submit", :class =>"btn btn-dark btn-sm" do %>
   <i class="fas fa-search"></i>
 <% end %>
<% end %>